<% if(accesorios[0][0].is_active == 1) { %>
    <input
        class="input input--full-width"
        type="text"
        id="status"
        name="status"
        placeholder="Ingresa el estado del accesorio"
        value="Activo"
        readonly
    />
<% } %>

<% if(accesorios[0][0].is_active == 2) { %>
    <input
        class="input input--full-width"
        type="text"
        id="status"
        name="status"
        placeholder="Ingresa el estado del accesorio"
        value="Inactivo"
        readonly
    />
<% } %>